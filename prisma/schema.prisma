// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Flight {
  id          Int      @unique @default(autoincrement())
  origin      String
  destination String
  departure   DateTime
  airline     String
  price       Float
  created_at  DateTime
  updated_at  DateTime

  @@map("flights")
}
